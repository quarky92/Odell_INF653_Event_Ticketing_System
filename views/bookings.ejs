    <table>
        <tr>
            <th>Title</th>
            <th>Venue</th>
            <th>Date</th>
            <th>Time</th>
            <th>Quantity</th>
        </tr>
        <% for(const booking of bookings) {%>
            <tr>
                <td><%=booking.event.title%></td>
                <td><%=booking.event.venue%></td>
                <td><%=booking.event.date.split('T')[0]%></td>
                <td><%=booking.event.time%></td>
                <td><%=booking.booking.quantity%></td>
                <td><a href="/booking/<%=booking.booking._id%>">Details</a></td>
            </tr>
        <%}%>
    </table>
